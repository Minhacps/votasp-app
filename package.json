{
  "name": "vota-sp",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "server": "http-server dist -p 80",
    "start": "NODE_ENV=dev parcel index.html",
    "build": "NODE_ENV=prod parcel build index.html",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./enzyme-setup.js",
    "moduleNameMapper": {
